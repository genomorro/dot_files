<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Command line fu</os:ShortName>
<os:Description>Grep the archive</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://www.commandlinefu.com/</SearchForm>
<os:Url type="text/html" method="POST" template="http://www.commandlinefu.com/search/handler">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="sourceid" value="Mozilla-search"/>
</os:Url><os:Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/commandlinefu.xml" rel="self">
</os:Url>
</SearchPlugin>